[] a rep:Repository;
  rep:repositoryID "wikidata-other-lookup";
  rdfs:label "SPARQL repository";
  rep:repositoryImpl [
      rep:repositoryType "metaphactory:SPARQLRepository";
      sparql:query-endpoint <https://query.wikidata.org/sparql>;
      mph:quadMode true
    ];
  lookup:configuration [
      lookup:type "metaphacts:sparqlLookup";
      lookup:datasetQuery """
        SELECT DISTINCT ?candidate ?dataset ?datasetLabel WHERE {
            VALUES(?dataset ?datasetLabel) {
                (<http://www.wikidata.org> \"wikidata\")
            }
        } 
      """;
      lookup:queryTemplate """
        SELECT
            ?candidate
            (GROUP_CONCAT(DISTINCT ?type ; separator=\",\") as ?types)
            (0.7 + ((20 - MAX(?score_private) + 1) / 100.0) as ?score)
        WHERE {
            {{type_block}}
            {{search_block}}
            {{properties_block}}
        } GROUP BY ?candidate ORDER BY ASC(?score_private) {{limit_block}}
      """;
      lookup:searchBlockTemplate """
      	{
          {SELECT ?candidate ?type ?score_private WHERE {
              SERVICE wikibase:mwapi {
                  bd:serviceParam wikibase:endpoint \"www.wikidata.org\";
                                  wikibase:api \"EntitySearch\";
                                  mwapi:language \"en\";
                                  mwapi:search ?__token__.
                  ?candidate wikibase:apiOutputItem mwapi:item.
                  ?score_private wikibase:apiOrdinal true.
              }
              ?candidate (wdt:P279|wdt:P31) ?type .
              ?type wdt:P279* ?mappedType .
              VALUES(?mappedType) {
    							( <http://www.wikidata.org/entity/Q16334295> )
                  ( <http://vocab.getty.edu/ontology#Concept> )
                  ( <http://www.wikidata.org/entity/Q811979> )
                  ( <http://www.wikidata.org/entity/Q2342494> )
                  ( <http://www.wikidata.org/entity/Q838948> )
                  ( <http://www.geonames.org/ontology#Feature> )
                  ( <https://d-nb.info/standards/elementset/gnd#Work> )
                  ( <https://d-nb.info/standards/elementset/gnd#BuildingOrMemorial> )
                  ( <http://vocab.getty.edu/ontology#GroupConcept> )
    
                  ( <http://www.cidoc-crm.org/cidoc-crm/E5_Event> )
                  ( <http://www.cidoc-crm.org/cidoc-crm/E53_Place> )
                  ( <http://www.cidoc-crm.org/cidoc-crm/E55_Type> )
                  ( <https://d-nb.info/standards/elementset/gnd#CorporateBody> )
                  ( <https://d-nb.info/standards/elementset/gnd#PlaceOrGeographicName> )
                  ( <http://iflastandards.info/ns/fr/frbr/frbroo/F24_Publication_Expression> )
                  ( <http://schema.swissartresearch.net/ontology/rds#crmArtwork> )
                  ( <http://schema.swissartresearch.net/ontology/rds#crmBuiltWork> )
                  ( <http://vocab.getty.edu/ontology#AdminPlaceConcept> )
                  ( <http://vocab.getty.edu/ontology#PhysAdminPlaceConcept> )
                  ( <http://vocab.getty.edu/ontology#PhysPlaceConcept> )
      						( <http://www.wikidata.org/entity/Q1190554> )
    						  ( <http://www.wikidata.org/entity/Q17350442> )
                  (  <http://www.wikidata.org/entity/Q18593264> )
                  (  <http://www.wikidata.org/entity/Q27096213> )
                  (  <http://www.wikidata.org/entity/Q47461344> )
                  ( <http://www.wikidata.org/entity/Q570116> )
                  ( <http://www.wikidata.org/entity/Q5946290> )
                  ( <http://www.wikidata.org/entity/Q6256> )
                  ( <http://www.wikidata.org/entity/Q732577> )
                  ( <http://www.wikidata.org/entity/Q8436> )
                  ( <https://d-nb.info/standards/elementset/gnd#ConferenceOrEvent> )
               }
        FILTER NOT EXISTS {
            ?candidate (wdt:P279|wdt:P31) ?badType .
            ?badType wdt:P279* ?badMappedType .
            VALUES(?badMappedType) {
            ( <http://vocab.getty.edu/ontology#PersonConcept> )
            (  <http://www.wikidata.org/entity/Q3778211> )
            ( <http://vocab.getty.edu/ontology#UnknownPersonConcept> )
            ( <https://d-nb.info/standards/elementset/gnd#Person> )
            ( <http://www.cidoc-crm.org/cidoc-crm/E21_Person> )
            ( <http://www.wikidata.org/entity/Q1415187> )
            ( <http://www.wikidata.org/entity/Q154954> )
            ( <http://www.wikidata.org/entity/Q5> )
            ( <http://www.wikidata.org/entity/Q215627> )
            ( <https://d-nb.info/standards/elementset/gnd#DifferentiatedPerson> )
            ( <https://d-nb.info/standards/elementset/gnd#UndifferentiatedPerson> )
            }

        }
          }}
        } UNION {
            BIND (1 as ?score_private)
            BIND (IRI(CONCAT(\"http://www.wikidata.org/entity/\", UCASE(?__token__))) as ?candidate)
            ?candidate (wdt:P279|wdt:P31) ?type .
        } UNION {
          BIND(IRI(?__token__) as ?candidate)
          ?candidate (wdt:P279|wdt:P31) ?type .
        }
    """;
      lookup:typeBlockTemplate """
